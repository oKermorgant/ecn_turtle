  <joint name="laser" type="fixed">
    <origin rpy="3.14159 0.0 0.0" xyz="0.15 0.0 0.360"/>
    <parent link="base_link"/>
    <child link="base_laser_link"/>
  </joint>
  <link name="base_laser_link">
    <visual>
      <geometry>
        <box size="0.05 0.1 0.06"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="0.000001"/>
      <origin xyz="0 0 0"/>
